    keywords = {Laborraum; Schüler; Interesse; Lernumgebung; Experiment; Lernort; Außerschulisches Lernen; Naturwissenschaften; Naturwissenschaftlicher Unterricht; Physikunterricht; Lehrangebot; Technik; Jugendlicher},
